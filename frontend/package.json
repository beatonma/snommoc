    "stylelint-config-recommended": "^16.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^4.0.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss",
      "@trivago/prettier-plugin-sort-imports"
    ],
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "^@/(.*)$",
      "^\\.+/.*$",
      "^(.*)\\.css$"
    ],
    "importOrderSortSpecifiers": true
  }
